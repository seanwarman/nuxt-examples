{
  "name": "tredz-component-library",
  "version": "0.8.32",
  "description": "Tredz component library",
  "main": "./dist/tredz-component-library.es.js",
  "module": "./dist/tredz-component-library.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/tredz-component-library.es.js",
      "require": "./dist/tredz-component-library.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "tailwind.config.js",
    "src/**/*.vue"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "testui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token=8b31d308676b --exit-zero-on-changes",
    "tw-compile": "npx tailwindcss -i ./src/main.css -o ./dist/main.css --watch"
  },
  "dependencies": {
    "@types/jsdom": "^20.0.0",
    "@vuelidate/core": "^2.0.0-alpha.43",
    "@vuelidate/validators": "^2.0.0-alpha.31",
    "fuse.js": "^6.6.2",
    "plyr": "^3.7.2",
    "swiper": "^8.3.1",
    "vue": "^3.2.33",
    "vue-final-modal": "^3.4.4",
    "vue-fuse": "^4.1.1",
    "vue-plyr": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@rushstack/eslint-patch": "^1.1.0",
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-docs": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/vue3": "^6.5.10",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vitest/ui": "^0.22.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.5",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.2",
    "chromatic": "^6.5.4",
    "eslint": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "lodash": "^4.17.21",
    "lodash.kebabcase": "^4.1.1",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "storybook-addon-designs": "^6.3.1",
    "storybook-vue3-router": "^2.3.1",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.34.7"
  },
  "resolutions": {
    "webpack": "^5"
  }
}
